<?xml version="1.0" encoding="UTF-8"?>
<gui title="*spectator-gui.title" rows="4">
    <staticpane x="0" y="0" length="9" height="4">
        <item id="feather" x="4" y="1" onLocalClick="flySpeedClick">
            <displayname>*spectator-gui.fly-speed.name</displayname>
            <lore>
                <line>*spectator-gui.fly-speed.lores</line>
            </lore>
        </item>
        <item id="book" x="4" y="2" onLocalClick="closeClick">
            <displayname>*spectator-gui.close-menu.name</displayname>
            <lore>
                <line>*spectator-gui.close-menu.lores</line>
            </lore>
        </item>
    </staticpane>
</gui>