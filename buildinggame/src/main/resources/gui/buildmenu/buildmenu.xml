<?xml version="1.0" encoding="UTF-8"?>
<gui title="*gui.options-title" rows="5">
    <staticpane x="0" y="0" length="9" height="5">
        <item id="*gui.particles.id" x="1" y="1" displayName="*gui.particles.name" lores="*gui.particles.lores"
              tag="particles" onClick="particlesClick" />
        <item id="*gui.floor.id" x="3" y="1" displayName="*gui.floor.name" lores="*gui.floor.lores" tag="floor"
              onClick="floorClick" />
        <item id="*gui.time.id" x="5" y="1" displayName="*gui.time.name" lores="*gui.time.lores" tag="time"
              onClick="timeClick" />
        <item id="*gui.rain.id" x="7" y="1" displayName="*gui.rain.name" lores="*gui.rain.lores" tag="rain"
              onClick="rainClick" />
        <item id="*gui.fly-speed.id" x="1" y="2" displayName="*gui.fly-speed.name" lores="*gui.fly-speed.lores"
              tag="fly-speed" onClick="flySpeedClick" />
        <item id="*gui.heads.id" x="3" y="2" displayName="*gui.heads.name" lores="*gui.heads.lores" tag="heads"
              onClick="headsClick" />
        <item id="*gui.banners.id" x="5" y="2" displayName="*gui.banners.name" lores="*gui.banners.lores" tag="banners"
              onClick="bannersClick" />
        <item id="*gui.biome.id" x="7" y="2" displayName="*gui.biome.name" lores="*gui.biome.lores" tag="biome"
              onClick="biomeClick" />
        <item id="book" x="4" y="3" displayName="*gui.close-menu.name" lores="*gui.close-menu.lores"
              onClick="closeMenuClick" />
    </staticpane>
</gui>